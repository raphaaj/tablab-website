{
  "name": "tablab-website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "prebuild": "npm run build:clean",
    "build": "next build",
    "build:clean": "rimraf .next",
    "start": "next start",
    "test": "jest",
    "pretest:coverage": "npm run test:coverage:clean",
    "test:coverage": "jest --coverage",
    "test:coverage:clean": "rimraf coverage",
    "lint": "next lint",
    "lint:fix": "npm run lint -- --fix",
    "prettier:check": "prettier --ignore-path .gitignore --check src/ tests/",
    "prettier:write": "prettier --ignore-path .gitignore --write src/ tests/"
  },
  "dependencies": {
    "@emotion/cache": "^11.11.0",
    "@emotion/react": "^11.11.3",
    "@emotion/server": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.15.11",
    "@mui/material": "^5.15.11",
    "ajv": "^8.12.0",
    "ajv-i18n": "^4.2.0",
    "ajv-keywords": "^5.1.0",
    "i18next": "^23.10.0",
    "i18next-fs-backend": "^2.3.1",
    "next": "^14.1.0",
    "next-i18next": "^15.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^14.0.5",
    "tablab": "^1.0.0-alpha.5",
    "tablab-i18n": "^1.0.0-alpha.2"
  },
  "devDependencies": {
    "@emotion/eslint-plugin": "^11.11.0",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/i18next-fs-backend": "^1.1.5",
    "@types/jest": "^29.5.12",
    "@types/react": "^18.2.58",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.1.0",
    "eslint-config-prettier": "^9.1.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "node-mocks-http": "^1.14.1",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
