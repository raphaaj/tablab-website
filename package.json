{
  "name": "tablab-website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "prebuild": "npm run build:clean",
    "build": "next build",
    "build:clean": "rimraf .next",
    "start": "next start",
    "test": "jest",
    "pretest:coverage": "npm run test:coverage:clean",
    "test:coverage": "jest --coverage",
    "test:coverage:clean": "rimraf coverage",
    "lint": "next lint",
    "lint:fix": "npm run lint -- --fix",
    "prettier:check": "prettier --ignore-path .gitignore --check src/ tests/",
    "prettier:write": "prettier --ignore-path .gitignore --write src/ tests/"
  },
  "dependencies": {
    "@emotion/cache": "^11.10.5",
    "@emotion/react": "^11.10.5",
    "@emotion/server": "^11.10.0",
    "@emotion/styled": "^11.10.5",
    "@mui/icons-material": "^5.11.0",
    "@mui/material": "^5.11.4",
    "@types/i18next-fs-backend": "^1.1.2",
    "ajv": "^8.12.0",
    "ajv-i18n": "^4.2.0",
    "ajv-keywords": "^5.1.0",
    "i18next": "^21.10.0",
    "i18next-fs-backend": "^1.2.0",
    "next": "^12.3.4",
    "next-i18next": "^12.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tablab": "^1.0.0-alpha.5",
    "tablab-i18n": "^1.0.0-alpha.2"
  },
  "devDependencies": {
    "@emotion/eslint-plugin": "^11.10.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.2.5",
    "@types/react": "^18.0.26",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "commitizen": "^4.2.6",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.32.0",
    "eslint-config-next": "^12.3.4",
    "eslint-config-prettier": "^8.6.0",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "node-mocks-http": "^1.12.1",
    "prettier": "^2.8.3",
    "rimraf": "^3.0.2",
    "typescript": "^4.9.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
